page {
  background-color: #f5f5f5;
}

.text {
  background-color: white;
  bottom: 0;
  position: fixed;
  width: 100%;
  height: 8%;
  box-shadow: 1rpx 2rpx 12rpx 2rpx rgb(216, 216, 216);
}

.box {
  border: 2rpx solid rgb(136, 223, 101);
  height: 60rpx;
  width: 180rpx;
  border-radius: 10rpx;
  color: rgb(136, 223, 101);
  display: flex;
  align-items: center;
  justify-content: center;
}

.top{
  margin-top: 150rpx;
}
